{
    "name": "petitpress/gps-messenger-bundle",
    "type": "symfony-bundle",
    "description": "Google Pub/Sub transport for Symfony Messenger",
    "keywords": ["messenger", "symfony", "google", "pubsub"],
    "homepage": "https://www.petitpress.sk",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Marfoldi",
            "email": "ronald.marfoldi@petitpress.sk"
        },
        {
            "name": "Petit Press, a.s.",
            "homepage": "https://www.petitpress.sk"
        },
        {
            "name": "PetitPressGpsMessengerBundle Community",
            "homepage": "https://github.com/petitpress/gps-messenger-bundle/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "psr/cache": "^1.0|^2.0|^3.0",
        "symfony/config": "^7.4|^8.0",
        "symfony/dependency-injection": "^7.4|^8.0",
        "symfony/http-kernel": "^7.4|^8.0",
        "symfony/messenger": "^7.4|^8.0",
        "symfony/options-resolver": "^7.4|^8.0",
        "google/cloud-pubsub": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/property-access": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/yaml": "^7.4|^8.0",
        "symplify/easy-coding-standard": "^11.1",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": { "PetitPress\\GpsMessengerBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "PetitPress\\GpsMessengerBundle\\Tests\\": "tests" }
    }
}
